{
  "name": "ng2-json-editor",
  "version": "0.4.0",
  "description": "Angular2 component for editing large json documents",
  "keywords": [
    "angular2",
    "typescript",
    "json",
    "marc",
    "json-editor",
    "ng2"
  ],
  "author": "Harun Urhan <harun.urhan@cern.ch>",
  "homepage": "https://github.com/inspirehep/ng2-json-editor",
  "license": "GPL-2.0",
  "angular-cli": {},
  "main": "ng2-json-editor.js",
  "scripts": {
    "build": "gulp build",
    "start": "npm run build && npm run serve",
    "serve": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test:unit": "ng test --watch=false",
    "test": "npm-run-all build test:unit e2e:travis",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "e2e:travis": "npm-run-all --parallel -r serve e2e:travis:delay",
    "e2e:travis:delay": "sleep 15 && npm run e2e",
    "preversion": "npm test",
    "version": "npm run build",
    "copy": "copyfiles LICENSE README.md package.json dist",
    "postversion": "git push upstream && git push upstream --tags",
    "coveralls": "cat coverage/coverage.lcov | coveralls",
    "typedoc": "typedoc --readme none --theme ./docs-theme --module commonjs --out ./example-dist/docs --tsconfig ./src/shared/interfaces/tsconfig.json ."
  },
  "peerDependencies": {
    "@angular/common": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "rxjs": "^5.0.1",
    "zone.js": "^0.7.2",
    "core-js": "^2.4.1",
    "ts-helpers": "^1.1.1"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash": "4.17.2",
    "mousetrap": "^1.6.0",
    "ng2-bootstrap": "1.3.0"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.30",
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.50",
    "@types/mousetrap": "^1.5.32",
    "@types/node": "^6.0.42",
    "bootstrap-sass": "^3.3.7",
    "codelyzer": "~2.0.0-beta.1",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "coveralls": "^2.11.12",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-ext-replace": "^0.3.0",
    "gulp-ngc": "^0.1.2",
    "gulp-sass": "^2.3.2",
    "gulp-tslint": "^7.0.1",
    "inline-ng2-resources": "1.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "npm-run-all": "^4.0.1",
    "process-nextick-args": "^1.0.7",
    "protractor": "^5.0.0",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typedoc": "^0.5.5",
    "typescript": "~2.0.3",
    "util-deprecate": "^1.0.2",
    "zone.js": "^0.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inspirehep/ng2-json-editor.git"
  },
  "bugs": {
    "url": "https://github.com/inspirehep/ng2-json-editor/issues"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3"
  }
}
