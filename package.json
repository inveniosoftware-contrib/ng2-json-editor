{
  "name": "ng2-json-editor",
  "version": "0.0.1-0",
  "description": "Angular2 component for editing large json documents",
  "keywords": [
    "angular2",
    "typescript",
    "json",
    "marc",
    "json-editor",
    "ng2"
  ],
  "author": "Harun Urhan <harun.urhan@cern.ch>",
  "homepage": "https://github.com/inspirehep/ng2-json-editor",
  "license": "GPL-2.0",
  "main": "ng2-json-editor.js",
  "scripts": {
    "tslint": "tslint",
    "typedoc": "typedoc",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pretest": "npm run lint",
    "test": "gulp test",
    "build": "gulp build",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "protractor": "protractor",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "start": "npm run build && webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base example/",
    "coveralls": "cat coverage/**/lcov.info | coveralls"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "core-js": "^2.4.1",
    "ng2-bootstrap": "1.0.24",
    "ng2-pdf-viewer": "0.0.7",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.13"
  },
  "devDependencies": {
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "awesome-typescript-loader": "^1.0.0",
    "bootstrap-sass": "^3.3.7",
    "codelyzer": "0.0.28",
    "copy-webpack-plugin": "^3.0.1",
    "coveralls": "^2.11.12",
    "del": "^2.2.2",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^3.0.1",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^6.1.1",
    "gulp-typescript": "^2.13.6",
    "html-webpack-plugin": "^2.17.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "merge2": "^1.0.2",
    "node-sass": "^3.8.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.2.2",
    "raw-loader": "0.5.1",
    "run-sequence": "^1.2.2",
    "sass-loader": "^4.0.0",
    "source-map-loader": "^0.1.5",
    "ts-helpers": "1.1.1",
    "ts-node": "^0.9.0",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.4.2",
    "typescript": "~1.8.9",
    "typings": "~1.0.5",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inspirehep/ng2-json-editor.git"
  },
  "bugs": {
    "url": "https://github.com/inspirehep/ng2-json-editor/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
