          }, 
          "value": {
            "type": "string"
          }
        }, 
        "type": "object"
      }, 
      "type": "array", 
      "uniqueItems": true
    }, 
    "imprints": {
      "items": {
        "properties": {
          "date": {
            "format": "date", 
            "type": "string"
          }, 
          "place": {
            "type": "string"
          }, 
          "publisher": {
            "type": "string"
          }
        }, 
        "type": "object"
      }, 
      "type": "array", 
      "uniqueItems": true
    }, 
    "isbns": {
      "items": {
        "properties": {
          "comment": {
            "description": "Further information about type.", 
            "type": "string"
          }, 
          "medium": {
            "description": "Medium (type) of the ISBN.", 
            "type": "string"
          }, 
          "value": {
            "format": "isbn", 
            "type": "string"
          }
        }, 
        "required": [
          "value"
        ], 
        "type": "object"
      }, 
      "type": "array", 
      "uniqueItems": true
    }, 
    "keywords": {
      "items": {
        "properties": {
          "classification_scheme": {
            "type": "string"
          }, 
          "keyword": {
            "type": "string"
          }, 
          "source": {
            "type": "string"
          }
        }, 
        "type": "object"
      }, 
      "type": "array", 
      "uniqueItems": true
    }, 
    "languages": {
