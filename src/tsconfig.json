{
  "__comment": "This is only used by IDE to avoid tsc errors",
  "compilerOptions": {
    "baseUrl": "../",
    "declaration": true,
    "stripInternal": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [
      "es6",
      "dom"
    ],
    "rootDir": "../",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "dist",
    "sourceMap": true,
